<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_boards">

    <fragment
        android:id="@+id/navigation_boards"
        android:name="com.theredspy15.thanelocker.ui.boards.BoardsFragment"
        android:label="Boards"
        tools:layout="@layout/fragment_boards" />

    <fragment
        android:id="@+id/navigation_sessions"
        android:name="com.theredspy15.thanelocker.ui.sessions.SessionsFragment"
        android:label="Sessions"
        tools:layout="@layout/fragment_sessions" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.theredspy15.thanelocker.ui.news.NewsFragment"
        android:label="News"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.theredspy15.thanelocker.ui.settings.SettingsFragment"
